doctype html
html
  head
    title Custom Object Table
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel="stylesheet", href="/css/style.css")
  body
    h1 Libros
    a(href="/update-cobj") Add to this table
    table(border="1")
      thead
        tr
          th ID
          th Name
          th Author
          th Genre
      tbody
        each record in records
          tr
            td= record.id
            td= record.properties.name
            td= record.properties.author
            td= record.properties.genre
